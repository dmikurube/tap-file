{
  "name": "Taps",
  "version": "0.1",
  "description": "Easy text input by taps",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },		    
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "background_page": "background.html",
  "content_scripts": [
  {
  "matches": ["<all_urls>"],
  "js": ["inject.js"]
  }
  ]
}